{
  "name": "express_code_structure",
  "version": "1.0.0",
  "description": "CMS app",
  "scripts": {
    "start": "nodemon ./app/server --exec babel-node --presets env | bistre",
    "release": "npm run clean && npm run build && npm run serve",
    "clean": "rm -rf dist && mkdir dist",
    "build": "babel . -s -D -d dist --presets env --ignore node_modules",
    "test": "./bin/test.sh",
    "lint": "standard"
  },
  "main": "./app/server",
  "engines": {
    "node": "10.x"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "express.js",
    "cms"
  ],
  "author": "Muhammad Ardie",
  "license": "MIT",
  "bugs": {
    "url": ""
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "bistre": "^1.0.1",
    "bole": "^4.0.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "express-validator": "^6.3.0",
    "formidable": "^1.2.1",
    "mongoose": "^5.8.0",
    "mv": "^2.1.1",
    "nodemon": "^2.0.1",
    "standard": "^14.3.1",
    "supertest": "^4.0.2",
    "tap-dot": "^2.0.0",
    "tape": "^4.11.0"
  }
}
